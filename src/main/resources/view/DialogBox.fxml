<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Circle?>
<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0"
         type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <StackPane fx:id="profPicPane" maxWidth="500">
            <Rectangle  fx:id="textBoxBorder" arcWidth="10" arcHeight="10"/>
            <Label fx:id="dialog" minHeight="-Infinity" text="Label" wrapText="true"/>

        </StackPane>
        <Circle fx:id="userImage" radius="40"/>


    </children>
    <padding>
        <Insets bottom="10.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</fx:root>
